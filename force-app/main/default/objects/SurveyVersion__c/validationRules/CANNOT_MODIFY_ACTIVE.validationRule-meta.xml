<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CANNOT_MODIFY_ACTIVE</fullName>
    <active>true</active>
    <errorConditionFormula>IF( ISPICKVAL(PRIORVALUE(Status__c), &quot;Active&quot;), TRUE, FALSE )</errorConditionFormula>
    <errorMessage>Activated Survey Versions cannot be modified. Create a new version.</errorMessage>
</ValidationRule>
